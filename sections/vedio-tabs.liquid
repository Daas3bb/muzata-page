<style>     
  .xai-tab .tab-icon {
    width: 24px;
    height: 24px;
    margin-right: 10px;
    vertical-align: middle;
    object-fit: contain;
  }

  .xai-tabs {
    display: flex;
    border-bottom: 2px solid #ccc;
    margin: 20px auto 0;
    justify-content: center;
    width: {{ section.settings.container_width }}px;
    overflow: hidden;
  }

  .xai-tab {
    padding: 10px 20px;
    cursor: pointer;
    font-size: 21px;
    transition: color 0.3s;
    width: 33.33%;
    text-align: center;
    color: #666;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
  }

  .xai-tab.xai-active {
    color: {{ section.settings.active_tab_color }};
    border-bottom: 7px solid;
    margin-bottom: -4px;
    border-image: linear-gradient(130deg, {{ section.settings.gradient_start_color }}, {{ section.settings.active_tab_color }}) 1;
  }

  .xai-tab:hover {
    color: #000;
  }

  /* Content Area Styles */
  .xai-tab-content {
    height: 0;
    overflow: hidden;
    opacity: 0;
    position: relative;
    margin: 10px auto;
    border-radius: 10px;
    box-sizing: border-box;
    max-width: {{ section.settings.container_width }}px;
    background-color: {{ section.settings.content_bg_color }};
    transition: all 0.5s ease-in-out;
  }

  .xai-tab-content.xai-active {
    display: block;
    height: auto;
    opacity: 1;
    overflow: visible;
  }

  /* Video Grid Container (Desktop) */
  .video-grid-container {
    display: flex;
    gap: 20px;
    align-items: flex-start;
    flex-wrap: wrap;
    margin-bottom: 30px;
  }

  .main-video-section {
    flex: 3.4;
    min-width: 300px;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    background-color: {{ section.settings.video_bg_color }};
    border-radius: 8px;
    overflow: hidden;
    box-shadow: 0 2px 5px rgba(0,0,0,0.1);
    padding-bottom: 20px;
  }

  .main-video-section .video-embed-wrapper {
    margin-bottom: 15px;
  }

  .side-videos-section {
    flex: 2;
    display: flex;
    flex-direction: column;
    gap: 20px;
    min-width: 250px;
    box-sizing: border-box;
  }

  /* Video Embed Wrapper */
  .video-embed-wrapper {
    position: relative;
    padding-bottom: 56.25%;
    height: 0;
    overflow: hidden;
    background-color: #000;
  }

  .video-embed-wrapper iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border: none;
  }

  /* Video Titles and Descriptions */
  .video-title {
    font-size: 22px;
    font-weight: 600;
    margin: 0 20px 10px;
    color: #333;
    text-align: left;
    text-wrap: auto;
    line-height:1.2;
  }

  .main-video-description {
    font-size: 18px;
    color: #666;
    line-height: 1.5;
    margin: 0 20px 20px;
    text-align: left;
    flex-grow: 1;
  }

  /* Side Video Item */
  .video-item {
    display: flex;
    align-items: stretch;
    gap: 15px;
    background-color: {{ section.settings.video_bg_color }};
    border-radius: 8px;
    overflow: hidden;
    box-shadow: 0 2px 5px rgba(0,0,0,0.1);
    flex-grow: 1;
  }

  .video-item .video-embed-wrapper {
    flex: 0 0 60%;
    padding-bottom: 33.75%;
    border-radius: 4px;
    overflow: hidden;
    position: relative;
  }

  .video-item-text-content {
    flex: 1;
    padding: 20px 0px;
    display: flex;
    flex-direction: column;
    justify-content: top;
  }

  .video-item .video-item-title {
    margin: 0;
    font-size: 15px;
    font-weight: 500;
    line-height: 1.2;
    color: #333;
  }

  .video-item .video-item-description {
    margin: 5px 0 0;
    font-size: 14px;
    color: #666;
    line-height: 1.4;
  }

  /* Button Styles */
  .explore-button-container {
    text-align: center;
    margin-top: 30px;
    margin-bottom: 20px;
  }

  .explore-more-button {
    background: {{ section.settings.button_bg_color }};
    color: {{ section.settings.button_text_color }} !important;
    border: none;
    padding: 10px 20px;
    font-size: 18px;
    border-radius: 10px;
    cursor: pointer;
    transition: background-color 0.3s;
    text-decoration: none;
    display: inline-block;
    box-shadow: 0 4px 8px rgba(0,0,0,0.1);
  }

  .explore-more-button:hover {
    filter: brightness(1.1);
    transform: translateY(-2px);
  }

  .section-title {
    text-align: center;
    font-size: 40px;
    color: #000;
    margin: 60px auto 30px;
    max-width: {{ section.settings.container_width }}px;
  }

  /* Mobile Swipe Container */
  .swipe-container {
    display: none;
    overflow-x: auto;
    scroll-snap-type: x mandatory;
    -webkit-overflow-scrolling: touch;
    white-space: nowrap;
    margin-bottom: 20px;
    scroll-behavior: smooth;
    scrollbar-width: none;
    -ms-overflow-style: none;
  }

  .swipe-container::-webkit-scrollbar {
    display: none;
  }

  .swipe-container .video-slide {
    display: inline-block;
    width: 100%;
    scroll-snap-align: start;
    padding: 0 10px;
    box-sizing: border-box;
    vertical-align: top;
    background-color: {{ section.settings.video_bg_color }};
    border-radius: 8px;
    box-shadow: 0 2px 5px rgba(0,0,0,0.1);
  }

  .swipe-container .video-slide .video-embed-wrapper {
    padding-bottom: 56.25%;
  }

  .swipe-container .video-slide .video-title,
  .swipe-container .video-slide .main-video-description,
  .swipe-container .video-slide .video-item-title,
  .swipe-container .video-slide .video-item-description {
    text-align: center;
    margin: 10px 0;
  }

  /* Progress Bar */
  .progress-bar {
    display: none;
    text-align: center;
    margin: 10px 0;
  }

  .progress-dot {
    display: inline-block;
    width: 10px;
    height: 10px;
    background-color: #ccc;
    border-radius: 50%;
    margin: 0 5px;
    transition: background-color 0.3s;
  }

  .progress-dot.active {
    background-color: {{ section.settings.active_tab_color }};
  }

  /* Video Cover Styles */
  .video-cover {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    cursor: pointer;
    background-color: #000; /* Fallback background */
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: hidden;
  }

  .video-cover-image {
    width: 100%;
    height: 100%;
    object-fit: cover; /* Ensures the image covers the area without distortion */
    transition: opacity 0.3s ease;
  }

  .video-cover:hover .video-cover-image {
    opacity: 0.8; /* Dim image on hover */
  }

  .video-cover .play-button {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 60px; /* Adjust size as needed */
    height: 60px; /* Adjust size as needed */
    background-color: rgb(0 0 0 / 60%); /* White with some transparency */
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    transition: transform 0.3s ease, background-color 0.3s ease;
    box-shadow: 0 0 15px rgba(0, 0, 0, 0.3);
  }

  .video-cover .play-button::before {
    content: '';
    border-style: solid;
    border-width: 15px 0 15px 25px; /* Creates a triangle */
    border-color: transparent transparent transparent #FFF; /* Black play arrow */
    margin-left: 5px; /* Adjust to center the triangle */
  }

  .video-cover .play-button:hover {
    transform: translate(-50%, -50%) scale(1.1); /* Slightly enlarge on hover */
    background-color: rgb(0 0 0 / 80%);;
  }

  /* Ensure iframe starts hidden when cover is present */
  .video-embed-wrapper iframe.lazy-video {
    display: none;
  }

  /* If you need to make sure the video is visible after JS plays it */
  .video-embed-wrapper iframe:not(.lazy-video-hidden) {
      display: block;
  }

  /* Media Queries for Mobile */
  @media (max-width: 768px) {
    p.video-item-description {
    margin: 0 !important;
}
