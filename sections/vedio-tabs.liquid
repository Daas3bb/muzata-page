<h2 class="section-title">{{ section.settings.section_title | escape }}</h2>
<div class="xai-tabs">
  {% for block in section.blocks %}
    {% if block.type == 'tab' %}
      <div class="xai-tab {% if forloop.first %}xai-active{% endif %}" data-tab="xai-tab-{{ block.settings.tab_id | escape }}">
        {% if block.settings.tab_icon %}
          <img src="{{ block.settings.tab_icon | img_url: '24x24' }}" alt="{{ block.settings.tab_title | escape }}" class="tab-icon">
        {% endif %}
        {{ block.settings.tab_title | escape }}
      </div>
    {% endif %}
  {% endfor %}
</div>

{% assign tabs_with_videos = section.blocks | where: 'type', 'tab' %}
{% assign videos = section.blocks | where: 'type', 'video' %}

{% for tab in tabs_with_videos %}
  <div class="xai-tab-content {% if forloop.first %}xai-active{% endif %}" id="xai-tab-{{ tab.settings.tab_id | escape }}">
    <div class="swipe-container">
      {% assign video_count = 0 %}
      {% for video in videos %}
        {% if video.settings.tab_id == tab.settings.tab_id %}
          {% assign video_count = video_count | plus: 1 %}
          <div class="video-slide">
            <div class="video-embed-wrapper">
              {% if video.settings.video_cover_image != blank %}
                <div class="video-cover" data-video-url="{{ video.settings.video_url | escape }}">
                  <img src="{{ video.settings.video_cover_image | img_url: 'master' }}" alt="{{ video.settings.video_title | escape }}" class="video-cover-image">
                  <div class="play-button"></div>
                </div>
                <iframe loading="lazy" class="lazy-video" data-src="{{ video.settings.video_url | escape }}" title="{{ video.settings.video_title | escape }}" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen style="display:none;"></iframe>
              {% else %}
                {% unless video.settings.video_url == blank %}
                  <iframe  loading="lazy" src="{{ video.settings.video_url | escape }}" title="{{ video.settings.video_title | escape }}" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
                {% endunless %}
              {% endif %}
            </div>
            {% if video.settings.video_title !=blank %}
              <h3 class="video-title">{{ video.settings.video_title | escape }}</h3>
            {% endif %}
            {% if video.settings.video_description %}
              <p class="main-video-description">{{ video.settings.video_description | escape }}</p>
            {% endif %}
            {% if video.settings.video_subtitle != blank %}
              <div class="video-item-text-content">
                <h3 class="video-item-title">{{ video.settings.video_subtitle | escape }}</h3>
                <p class="video-item-description">{{ video.settings.video_subdescription | escape }}</p>
              </div>
            {% endif %}
          </div>
        {% endif %}
      {% endfor %}
    </div>
    <div class="progress-bar">
      {% for i in (1..video_count) %}
        <span class="progress-dot {% if forloop.first %}active{% endif %}"></span>
      {% endfor %}
    </div>
    <div class="video-grid-container">
      <div class="main-video-section">
        {% for video in videos %}
          {% if video.settings.tab_id == tab.settings.tab_id and video.settings.is_main_video %}
            <div class="video-embed-wrapper">
              {% unless video.settings.video_url == blank %}
                {% if video.settings.video_cover_image != blank %}
                  <div class="video-cover" data-video-url="{{ video.settings.video_url | escape }}">
                    <img src="{{ video.settings.video_cover_image | img_url: 'master' }}" alt="{{ video.settings.video_title | escape }}" class="video-cover-image">
                    <div class="play-button"></div>
                  </div>
                  <iframe  loading="lazy" class="lazy-video" data-src="{{ video.settings.video_url | escape }}" title="{{ video.settings.video_title | escape }}" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen style="display:none;"></iframe>
                {% else %}
                  <iframe  loading="lazy" src="{{ video.settings.video_url | escape }}" title="{{ video.settings.video_title | escape }}" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
                {% endif %}
              {% endunless %}
            </div>
            {% if video.settings.video_title != blank%}
              <h3 class="video-title">{{ video.settings.video_title | escape }}</h3>
            {% endif %}
            {% if video.settings.video_description %}
              <p class="main-video-description">{{ video.settings.video_description | escape }}</p>
            {% endif %}
          {% endif %}
        {% endfor %}
      </div>
      <div class="side-videos-section">
        {% for video in videos %}
          {% if video.settings.tab_id == tab.settings.tab_id and video.settings.is_main_video == false %}
            <div class="video-item">
              <div class="video-embed-wrapper">
                {% unless video.settings.video_url == blank %}
                  {% if video.settings.video_cover_image != blank %}
                    <div class="video-cover" data-video-url="{{ video.settings.video_url | escape }}">
                      <img src="{{ video.settings.video_cover_image | img_url: 'master' }}" alt="{{ video.settings.video_subtitle | escape }}" class="video-cover-image">
                      <div class="play-button"></div>
                    </div>
                    <iframe  loading="lazy" class="lazy-video" data-src="{{ video.settings.video_url | escape }}" title="{{ video.settings.video_subtitle | escape }}" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen style="display:none;"></iframe>
                  {% else %}
                    <iframe  loading="lazy" src="{{ video.settings.video_url | escape }}" title="{{ video.settings.video_subtitle | escape }}" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
                  {% endif %}
                {% endunless %}
              </div>
              <div class="video-item-text-content">
                {% if video.settings.video_subtitle != blank%}
                  <h3 class="video-item-title">{{ video.settings.video_subtitle | escape }}</h3>
                {% endif %}
                {% if video.settings.video_subdescription %}
                  <p class="video-item-description">{{ video.settings.video_subdescription | escape }}</p>
                {% endif %}
              </div>
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>
    <div class="explore-button-container">
      {% unless tab.settings.button_link == blank %}
        <a href="{{ tab.settings.button_link | escape }}" class="explore-more-button">{{ tab.settings.button_text | escape }}</a>
      {% endunless %}
    </div>
  </div>
{% endfor %}

<script>
  // 标签切换和滑动导航
const xaiTabs = document.querySelectorAll('.xai-tab');
const xaiTabContents = document.querySelectorAll('.xai-tab-content');
const xaiTabsContainer = document.querySelector('.xai-tabs');

xaiTabs.forEach((xaiTab, index) => {
  xaiTab.addEventListener('click', () => {
    xaiTabs.forEach(t => t.classList.remove('xai-active'));
    xaiTabContents.forEach(content => content.classList.remove('xai-active'));

    xaiTab.classList.add('xai-active');
    const tabId = xaiTab.getAttribute('data-tab');
    if (tabId) {
      const tabContent = document.getElementById(tabId);
      if (tabContent) {
        tabContent.classList.add('xai-active');
      } 
    } 

    if (window.innerWidth <= 768) {
      const tabWidth = xaiTab.offsetWidth;
      const translateX = -index * tabWidth * 0.4;
      xaiTabsContainer.style.transform = `translateX(${translateX}px)`;
    }
  });
});

// 滑动容器和进度条逻辑
const swipeContainers = document.querySelectorAll('.swipe-container');
swipeContainers.forEach(container => {
  if (container.nextElementSibling) {
    const progressDots = container.nextElementSibling.querySelectorAll('.progress-dot');
    container.addEventListener('scroll', () => {
      const slideWidth = container.clientWidth;
      const scrollPosition = container.scrollLeft;
      const currentSlide = Math.round(scrollPosition / slideWidth);
      progressDots.forEach((dot, index) => {
        dot.classList.toggle('active', index === currentSlide);
      });
    });
  } 
});

// 窗口大小调整时重置标签位置
window.addEventListener('resize', () => {
  if (window.innerWidth > 768) {
    xaiTabsContainer.style.transform = 'translateX(0)';
  } else {
    const activeTab = document.querySelector('.xai-tab.xai-active');
    if (activeTab) {
      const index = Array.from(xaiTabs).indexOf(activeTab);
      const tabWidth = activeTab.offsetWidth;
      const translateX = -index * tabWidth * 0.8;
      xaiTabsContainer.style.transform = `translateX(${translateX}px)`;
    } 
  }
});

// 为播放按钮绑定视频播放逻辑
const playButtons = document.querySelectorAll('.play-button');

playButtons.forEach(button => {
  // 获取父级的 video-cover 和对应的 iframe
  const videoCover = button.closest('.video-cover');
  const iframe = videoCover?.nextElementSibling;

  if (videoCover && iframe) {
    button.addEventListener('click', () => {
      let videoUrl = videoCover.dataset.videoUrl;
      
      // 确保 videoUrl 包含 autoplay=1 参数
      if (videoUrl && !videoUrl.includes('autoplay=1')) {
        const separator = videoUrl.includes('?') ? '&' : '?';
        videoUrl = `${videoUrl}${separator}autoplay=1&mute=0`;
      }

      if (videoUrl) {
        // 设置 iframe 的 src 并显示
        iframe.src = videoUrl;
        iframe.classList.remove('lazy-video'); // 移除隐藏类
        iframe.style.display = 'block';
        videoCover.style.display = 'none'; // 隐藏封面

        // 确保 iframe 允许自动播放
        iframe.setAttribute('allow', 'autoplay; encrypted-media; picture-in-picture');

        
      } 
    });
  } 
});
</script>

